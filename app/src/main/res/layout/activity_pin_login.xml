<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.datadissolve.ui.pin_login">

    <EditText
        android:id="@+id/pinEntry"
        android:layout_width="106dp"
        android:layout_height="45dp"
        android:layout_marginStart="152dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="153dp"
        android:layout_marginBottom="670dp"
        android:autofillHints=""
        android:inputType="numberPassword"
        android:maxLength="6"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="LabelFor" />

    <Button
        android:id="@+id/loginButton"
        style="@style/NumericButton"
        android:layout_width="132dp"
        android:layout_height="75dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="582dp"
        android:text="@string/login_button"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button7"
        style="@style/NumericButton"
        android:layout_width="87dp"
        android:layout_height="71dp"
        android:layout_marginStart="256dp"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="330dp"
        android:onClick="onNumericButtonClick"
        android:text="9"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button5"
        app:layout_constraintVertical_bias="0.2"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/button"
        style="@style/NumericButton"
        android:layout_marginStart="160dp"
        android:layout_marginBottom="245dp"
        android:onClick="onNumericButtonClick"
        android:text="0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button3"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/clear_button"
        style="@style/NumericButton"
        android:layout_width="115dp"
        android:layout_height="79dp"
        android:layout_marginTop="582dp"
        android:layout_marginEnd="80dp"
        android:text="@string/clear_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button3"
        style="@style/NumericButton"
        android:layout_width="87dp"
        android:layout_height="71dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="9dp"
        android:layout_marginBottom="12dp"
        android:onClick="onNumericButtonClick"
        android:text="8"
        app:layout_constraintBottom_toTopOf="@+id/button"
        app:layout_constraintEnd_toStartOf="@+id/button7"
        app:layout_constraintTop_toBottomOf="@+id/button4"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/button8"
        style="@style/NumericButton"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="8dp"
        android:onClick="onNumericButtonClick"
        android:text="7"
        app:layout_constraintEnd_toStartOf="@+id/button3"
        app:layout_constraintTop_toBottomOf="@+id/button9"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/button5"
        style="@style/NumericButton"
        android:layout_marginStart="258dp"
        android:layout_marginTop="20dp"
        android:onClick="onNumericButtonClick"
        android:text="6"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button6"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/button4"
        style="@style/NumericButton"
        android:layout_marginStart="160dp"
        android:layout_marginTop="20dp"
        android:onClick="onNumericButtonClick"
        android:text="5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button2"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/button9"
        style="@style/NumericButton"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="8dp"
        android:onClick="onNumericButtonClick"
        android:text="4"
        app:layout_constraintEnd_toStartOf="@+id/button4"
        app:layout_constraintTop_toBottomOf="@+id/button10"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/button6"
        style="@style/NumericButton"
        android:layout_width="87dp"
        android:layout_height="69dp"
        android:layout_marginStart="258dp"
        android:layout_marginBottom="512dp"
        android:onClick="onNumericButtonClick"
        android:text="3"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/button2"
        style="@style/NumericButton"
        android:layout_width="87dp"
        android:layout_height="70dp"
        android:layout_marginStart="160dp"
        android:layout_marginBottom="511dp"
        android:onClick="onNumericButtonClick"
        android:text="2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/button10"
        style="@style/NumericButton"
        android:layout_width="86dp"
        android:layout_height="70dp"
        android:layout_marginStart="66dp"
        android:layout_marginBottom="512dp"
        android:onClick="onNumericButtonClick"
        android:text="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="HardcodedText" />

</androidx.constraintlayout.widget.ConstraintLayout>